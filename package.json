{
  "name": "noteum",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "AI-powered note-taking application with local-first storage",
  "scripts": {
    "dev": "vite dev --port 3000",
    "dev:tauri": "tauri dev",
    "build": "vite build",
    "build:tauri": "tauri build",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
